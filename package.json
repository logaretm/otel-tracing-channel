{
  "name": "otel-tracing-channel",
  "version": "0.2.0",
  "description": "A fixed wrapper around Node.js's tracingChannel that properly propagates otel context",
  "type": "module",
  "license": "Apache-2.0",
  "homepage": "https://github.com/logaretm/otel-tracing-channel#readme",
  "bugs": {
    "url": "https://github.com/logaretm/otel-tracing-channel/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logaretm/otel-tracing-channel.git"
  },
  "author": "Abdelrahman Awad <me@awad.dev>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cjs",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "lint": "oxlint --type-aware ./",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm lint && pnpm build"
  },
  "devDependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@types/node": "^24.9.1",
    "bumpp": "^10.3.1",
    "oxlint": "^1.25.0",
    "oxlint-tsgolint": "^0.4.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.9",
    "typescript": "^5.9.3",
    "vitest": "^4.0.1"
  }
}
